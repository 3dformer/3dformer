#header
  text-align: left
  padding: 10px
  display: flex
  justify-content: space-around
  #logo
    width: 60px
  h1
    margin: 0
    display: inline-flex
    align-items: center

#header-nav-container
  display: inline-flex
  align-items: center
  ul
    font-size: 1.2em
    list-style: none
    display: inline-flex
    margin: auto 0
    justify-content: space-between
    width: 300px
    a
      color: #FFF
      text-decoration: none
